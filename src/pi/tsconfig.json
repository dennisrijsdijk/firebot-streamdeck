{
  "compilerOptions": {
    "customConditions": [
      "browser"
    ],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "es2022"
    ],
    "module": "es2022",
    "moduleResolution": "Bundler",
    "noImplicitOverride": true,
    "skipLibCheck": true,
    "target": "es2022",
  },
  "include": [
    "./"
  ]
}
